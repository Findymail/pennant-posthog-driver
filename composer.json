{
    "name": "findymail/pennant-posthog-driver",
    "description": "A pennant Posthog driver implementation",
    "version": "1.1.4",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Findymail\\PennantPosthogDriver\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jonathan Recly",
            "email": "jonathan@findymail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/pennant": "^1.10",
        "posthog/posthog-php": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Findymail\\PennantPosthogDriver\\Providers\\PennantPosthogDriverServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
